# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

#ignore generated testing output
/coverage

#ignore files in the gedifiles directory
/gedifiles/*

#ignore logs in the ftp server directory
/apache-ftpserver-1.0.5/res/log/*

#ignore uploaded files in the ftp server home directory
/apache-ftpserver-1.0.5/res/home/*

#ignore config files
/config/database.yml
/config/gedi.yml
/config/mailer.yml
/config/ftp.yml

#ignore mailer configs
/app/mailers/gedi_mailer.rb
/config/initializers/setup_mailer.rb

